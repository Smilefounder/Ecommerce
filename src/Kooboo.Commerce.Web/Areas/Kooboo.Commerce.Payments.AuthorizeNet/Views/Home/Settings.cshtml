@model Kooboo.Commerce.Payments.AuthorizeNet.AuthorizeNetSettings
@{
    Layout = "~/Areas/Commerce/Views/Shared/AddIns/_PaymentProcessorSettings.cshtml";
}

<div class="block common-form">
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary(true)
        <table>
            <tbody>
                @Html.EditorFor(m => m.LoginId, new { HtmlAttributes = new { @class = "medium" } })
                @Html.EditorFor(m => m.TransactionKey, new { HtmlAttributes = new { @class = "medium" } })
                @Html.EditorFor(m => m.SandboxMode)
            </tbody>
        </table>
    }
</div>
