@using Kooboo.Commerce.Customers
@using Kooboo.Commerce.Web.Framework.UI.Toolbar
@{
    ViewBag.Title = "Customers".Localize();
    ViewBag.EnableSearch = true;
}

@section Panel {
    @(Html.HeaderPanel().Buttons(buttons =>
    {
        buttons.AddCreateButton();
        buttons.AddDeleteButton();
        buttons.AddToolbarCommands(ToolbarCommands.GetCommands(ViewContext.Controller.ControllerContext));
    }))
}

@Html.Partial("_QueryGrid")

