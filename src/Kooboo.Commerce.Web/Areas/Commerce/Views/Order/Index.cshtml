@using Kooboo.Commerce.Orders
@using Kooboo.Commerce.Web.Framework.Actions
@{
    ViewBag.Title = "Orders".Localize();
    ViewBag.EnableSearch = true;
}

@section Panel {
    @(Html.HeaderPanel().Buttons(buttons =>
    {
        buttons.AddCreateButton();
        buttons.AddDeleteButton();
        buttons.AddEntityActions(Actions.GetActions(typeof(Order)));
    }))
}

@Html.Partial("_QueryGrid")