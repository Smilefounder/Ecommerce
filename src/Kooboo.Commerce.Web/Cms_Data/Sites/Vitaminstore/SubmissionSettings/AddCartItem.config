	<Settings xmlns:d2p1="http://schemas.microsoft.com/2003/10/Serialization/Arrays">
		<d2p1:KeyValueOfstringstring>
			<d2p1:Key>ProductPriceId</d2p1:Key>
			<d2p1:Value>{ProductPriceId}</d2p1:Value>
